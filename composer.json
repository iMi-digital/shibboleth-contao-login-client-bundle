        "contao/test-case": "^4.6",
        "phpunit/phpunit": "^9.3",
        "symfony/phpunit-bridge": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\SwissAlpineClubContaoLoginClientBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\SwissAlpineClubContaoLoginClientBundle\\ContaoManager\\Plugin"
